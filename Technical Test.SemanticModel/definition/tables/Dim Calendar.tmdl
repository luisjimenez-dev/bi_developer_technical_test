				    #"Added Month Number" = Table.AddColumn(#"Added Year", "Month Number", each Date.Month([Date]), Int64.Type),
				    #"Added Day" = Table.AddColumn(#"Added Month Number", "Day", each Date.Day([Date]), Int64.Type),
				    #"Added Month Short" = Table.AddColumn(#"Added Day", "Month Short", each Date.ToText([Date], "MMM", "en-US"), type text),
				    #"Added Month Long" = Table.AddColumn(#"Added Month Short", "Month Long", each Date.ToText([Date], "MMMM", "en-US"), type text),
				    #"Added YearMonthNum" = Table.AddColumn(#"Added Month Long", "Year Month Num", each [Year]*100 + [Month Number], Int64.Type),
				    #"Added YearMonthShort" = Table.AddColumn(#"Added YearMonthNum", "Year Month Short", each [Month Short] & " " & Text.End(Text.From([Year]), 2), type text),
				    #"Added YearMonthLong" = Table.AddColumn(#"Added YearMonthShort", "Year Month Long", each [Month Long] & " " & Text.From([Year]), type text),
				    #"Added DayOfWeekNum" = Table.AddColumn(#"Added YearMonthLong", "Day Of Week Num", each Date.DayOfWeek([Date], Day.Monday) + 1, Int64.Type),
				    #"Added DayOfWeekName" = Table.AddColumn(#"Added DayOfWeekNum", "Day Of Week Name", each Date.ToText([Date], "dddd", "en-US"), type text),
				    #"Added IsWeekend" = Table.AddColumn(#"Added DayOfWeekName", "Is Weekend", each if [Day Of Week Num] > 5 then true else false, type logical),
				    #"Added Quarter" = Table.AddColumn(#"Added IsWeekend", "Quarter", each Date.QuarterOfYear([Date]), Int64.Type),
				    #"Added Week" = Table.AddColumn(#"Added Quarter", "Week", each Date.WeekOfYear([Date]), Int64.Type),
				    #"Added EndOfMonth" = Table.AddColumn(#"Added Week", "End Of Month", each Date.EndOfMonth([Date]), type date),

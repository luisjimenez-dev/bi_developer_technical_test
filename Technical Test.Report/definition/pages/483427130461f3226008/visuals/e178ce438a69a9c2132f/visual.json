                  {
                    "value": "do not use the natural keys",
                    "textStyle": {
                      "fontWeight": "bold",
                      "fontSize": "10.5pt"
                    }
                  },
                  {
                    "value": " provided in the source files for products and users. Instead, we generate our own keys. Any missing product or user is assigned an ",
                    "textStyle": {
                      "fontSize": "10.5pt"
                    }
                  },
                  {
                    "value": "“Unknown”",
                    "textStyle": {
                      "fontWeight": "bold",
                      "fontSize": "10.5pt"
                    }
                  },
                  {
                    "value": " value to ensure referential integrity.",
                    "textStyle": {
                      "fontSize": "10.5pt"
                    }
                  }
                ],
                "listType": "bullet"
